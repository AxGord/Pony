{
	"config": "airmobile",
	"airOptions": {
		"output": "::OUTPUT::::APP::.air",
        "android": {
            "output": "::OUTPUT::::APP::.apk",
            "signingOptions": {
                "storetype": "pkcs12",
                "keystore": "::SERT::"
            },
            "listen": true
        }
    },
    "application": "air-app.xml",
	"compilerOptions": {
		"source-path": [
			"src"
		],
        "output": "::OUTPUT::::APP::.swf",
        "debug": true,
        "default-frame-rate": 24,
        "use-network": true
    },
    "mainClass": "Main"
}