extern class Sys{
	static public print(method) : v : Dynamic -> Void;

	static public println(method) : v : Dynamic -> Void;

	static public args(method) : Void -> Array<String>;

	static public getEnv(method) : s : String -> String;

	static public putEnv(method) : s : String -> v : String -> Void;

	static public environment(method) : Void -> haxe.ds.StringMap<String>;

	static public sleep(method) : seconds : Float -> Void;

	static public setTimeLocale(method) : loc : String -> Bool;

	static public getCwd(method) : Void -> String;

	static public setCwd(method) : s : String -> Void;

	static public systemName(method) : Void -> String;

	static public command(method) : cmd : String -> ?args : Array<String> -> Int;

	static public exit(method) : code : Int -> Void;

	static public time(method) : Void -> Float;

	static public cpuTime(method) : Void -> Float;

	static public executablePath(method) : Void -> String;

	static public getChar(method) : echo : Bool -> Int;

	static public stdin(method) : Void -> haxe.io.Input;

	static public stdout(method) : Void -> haxe.io.Output;

	static public stderr(method) : Void -> haxe.io.Output;

}